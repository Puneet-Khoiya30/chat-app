{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.js",
             "methods": ["GET","POST","OPTIONS","PUT","DELETE","PATCH"],
             "headers": {
              "Access-Control-Allow-Origin": "*"
             }
        }
    ]
}